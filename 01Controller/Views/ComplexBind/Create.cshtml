
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Create</title>
</head>
<body>
    <div>
        <h2>產品資料新增作業</h2>
        <form action="/ComplexBind/Create" method="post">
            <p>品號: <input id="PId" name="PId" type="text" /></p>
            <p>品名: <input id="PName" name="PName" type="text" /></p>
            <p>單價: <input id="Price" name="Price" type="text" /></p>
            <p><input id="Submit1" type="submit" value="新增商品" /></p>
        </form>
        <hr />
        <h3>新增作業完成</h3>
        <p>內容如下</p>
        <p>品號: @ViewBag.PId</p>
        <p>品名: @ViewBag.PName</p>
        <p>單價: @ViewBag.Price</p>
    </div>
</body>
</html>
